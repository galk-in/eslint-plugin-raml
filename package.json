{
  "name": "eslint-plugin-raml",
  "version": "0.2.7",
  "description": "Eslint rules for RAML",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "RAML"
  ],
  "files": [
    "lib"
  ],
  "author": "Nikita Galkin",
  "main": "lib/index.js",
  "scripts": {
    "cs": "eslint lib/**/**/*.js test/**/**/*.js",
    "cs-fix": "eslint lib/** test/** --fix",
    "test": "npm run cs && npm run test-no-cs",
    "test-no-cs": "mocha",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "dependencies": {
    "raml-1-parser": "1.1.14",
    "raml-json-validation": "0.0.11"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.14",
    "eslint": "^3.8.0",
    "eslint-config-xo-space": "^0.15.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2"
  },
  "engines": {
    "node": ">=4"
  },
  "eslintConfig": {
    "extends": "xo-space",
    "env": {
      "mocha": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/galk-in/eslint-plugin-raml.git"
  },
  "bugs": {
    "url": "https://github.com/galk-in/eslint-plugin-raml/issues"
  },
  "url": "https://github.com/galk-in/eslint-plugin-raml",
  "license": "MIT"
}
